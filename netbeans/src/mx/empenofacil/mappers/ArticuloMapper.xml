<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="articulo">
    
    <insert id="comercializar" parameterType="mx.empenofacil.beans.Articulo">
        INSERT INTO empenofacil.articulo
        (prenda,empleado,precio,descuento,codigodebarras,nombre,descripcion)
        VALUES(#{prenda.idPrenda},#{empleado.idempleado},#{precio},0,#{codigoDeBarras},#{nombre},#{descripcion});
    </insert>
    
</mapper>