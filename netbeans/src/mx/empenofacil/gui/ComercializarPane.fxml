      <HBox alignment="CENTER_RIGHT" fillHeight="false" spacing="10.0">
         <children>
            <Button fx:id="btn_aceptar" defaultButton="true" mnemonicParsing="false" onAction="#aceptar" text="Ejecutar" />
            <Button fx:id="btn_cancelar" cancelButton="true" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
         </children>
      </HBox>
   </children>
